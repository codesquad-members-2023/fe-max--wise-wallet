{
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "object-curly-newline": 0,
    "prefer-const": 0,
    "no-param-reassign": 0,
    "import/prefer-default-export": 0,
    "import/extensions": 0,
    "no-restricted-syntax": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es2021": true
  }
}